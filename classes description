### Team

  attributes:
    + name: string
    + guard: Player
    + guard2: Player
    + forward1: Player
    + forward2: Player
    + center: Player
    + managerName: string
    + availability: bool
    + static teams: Team[]
  
  methodes:
    + static CreateAllTeams(): void
    + static PrintAllTeams(): void
    + getTeamName(): string
    
    
### Player

  attributes:
    + name: string
    + position: string
    + points: double
    + rebounds: double
    + assists: double
    + stills: double
    + blocks: double
    + basketball_iq: int
    + bpm: double
    + team: string
    + salary: double
    + availabilty: bool
    + static players: Player[]   
  
  methodes:
    + static CreateAllPlayers(): void
    + static PrintAllPlayers(): void
    + static electRandomPlayer(): Player
